package com.etc._HomeWork;
/**
 * 娑堣垂鑰�
 * @author Administrator
 *
 */
public class Consumer  implements Runnable {
	//鍏变韩璧勬簮鐨勫璞�
	private ShareResoure share = null;
	public Consumer(ShareResoure share){
		this.share = share;
	}
	@Override
	public void run() {
		// TODO Auto-generated method stub
		for(int i=0;i<50;i++){
			share.pop();
		}
	}

}
